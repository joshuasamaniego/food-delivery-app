     )}\n      </MenuColumn>\n      <TotalColumn>\n        <h2>\n          {current\n            ? `Your Order From ${current.restaurant_name}`\n            : \"Your Order\"}\n        </h2>\n        <div>\n          {cart &&\n            cart.map((item) => {\n              return (\n                <CartDiv key={item.name}>\n                  <p>{item.name}</p>\n                  <div>\n                    <p>{item.quantity}</p>\n                    <Incrementer>\n                      <p onClick={() => dispatch(decrease(item))}>-</p>\n                      <p onClick={() => dispatch(increase(item))}>+</p>\n                    </Incrementer>\n                  </div>\n                  <p>{item.price}</p>\n                  <Exit onClick={() => removeFromCart(item)}>\n                    <img\n                      src=\"/assets/icon-close.svg\"\n                      alt=\"remove item from cart\"\n                    />\n                  </Exit>\n                </CartDiv>\n              );\n            })}\n        </div>\n        <Total>\n          <h2>{total === 0 ? \"Total: $0.00\" : `Total: $${total}`}</h2>\n        </Total>\n        <Checkout\n          onClick={() => {\n            if (total > 0) {\n              placeOrder(cart, current, total);\n            }\n          }}\n        >\n          Place Order\n        </Checkout>\n      </TotalColumn>\n    </OrderDiv>\n  );\n}\n\nconst OrderDiv = styled.div`\n  display: flex;\n  width: 100vw;\n`;\n\nconst RestColumn = styled.div`\n  width: 25%;\n  background: #f2f2f2;\n  box-shadow: 7px 7px 9px #d7d7d7, -7px -7px 9px #ffffff;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  overflow-y: scroll;\n\n  img {\n    width: 100px;\n    height: 100px;\n    margin-top: 10px;\n    filter: drop-shadow(-3px -3px 8px #6fd6ff);\n  }\n\n  input {\n    width: 80%;\n    height: 40px;\n    margin-top: 15px;\n    margin-bottom: 15px;\n    border: none;\n    border-radius: 12px;\n    background: #f2f2f2;\n